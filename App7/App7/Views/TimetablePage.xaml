<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App7.Views.TimetablePage"
             xmlns:vm="clr-namespace:App7.ViewModels"
             Title="{Binding Title}">
    <ContentPage.BindingContext>
        <vm:TimetableViewModel/>
    </ContentPage.BindingContext>
    <ContentPage.Content>
        <StackLayout>
            <WebView Source="https://tpt.siseveeb.ee/veebivormid/tunniplaan"
                     HeightRequest="1000"
                     WidthRequest="1000" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>